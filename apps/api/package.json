{
  "name": "@app/api",
  "version": "0.1.0",
  "description": "NestJS API service",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier -w \"src/**/*.ts\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"knexfile.js\"",
    "format:check": "prettier -c \"src/**/*.ts\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"knexfile.js\"",
    "format:watch": "prettier -w --watch \"src/**/*.ts\" \"migrations/**/*.js\" \"seeds/**/*.js\" \"knexfile.js\"",
    "migration:make": "knex migrate:make --knexfile knexfile.js",
    "migration:latest": "knex migrate:latest --knexfile knexfile.js",
    "migration:rollback": "knex migrate:rollback --knexfile knexfile.js",
    "seed:make": "knex seed:make --knexfile knexfile.js",
    "seed:run": "knex seed:run --knexfile knexfile.js"
  },
  "dependencies": {
    "@nestjs/axios": "^3.1.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.1.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "knex": "^3.1.0",
    "nest-winston": "^1.10.0",
    "objection": "^3.1.3",
    "pg": "^8.11.5",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "ts-jest": "^29.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=24"
  }
}
